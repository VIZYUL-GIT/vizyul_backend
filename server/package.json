{
  "name": "vizyul",
  "version": "1.0.0",
  "description": "Vizyul backend",
  "main": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test:watch": "jest --watch",
    "dev:test": "NODE_ENV=test npm run dev",
    "start:test": "NODE_ENV=test npm run start",
    "lint": "eslint config controllers models routes",
    "lint-fix": "bash fixTab.sh && eslint config controllers models routes --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VIZYUL-GIT/vizyul_backend"
  },
  "keywords": [
    "vizyul",
    "backend"
  ],
  "author": "superwebtop",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VIZYUL-GIT/vizyul_backend/issues"
  },
  "homepage": "https://github.com/VIZYUL-GIT/vizyul_backend#readme",
  "dependencies": {
    "base64-img": "^1.0.4",
    "bcrypt": "^3.0.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dom": "0.0.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "http-proxy-middleware": "^0.19.1",
    "jsonwebtoken": "^8.5.0",
    "mime": "^2.4.0",
    "mime-types": "^2.1.22",
    "moment": "^2.24.0",
    "mongoose": "^5.4.13",
    "mongoose-findorcreate": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-linkedin-oauth2": "^1.6.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg-monitor": "^1.1.0",
    "pg-promise": "^8.7.2",
    "sequelize": "^4.42.0",
    "uuid": "^3.3.2",
    "xml2js": "^0.4.19",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "superagent": "^5.0.5",
    "supertest": "^4.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/coverage",
      "/client/",
      "/.git",
      "/.idea"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/coverage",
      "/client/",
      "/.git",
      "/.idea"
    ],
    "testEnvironment": "node",
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
